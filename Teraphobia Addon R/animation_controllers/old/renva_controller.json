{
  "format_version": "1.8.0",
  "animation_controllers": {
    "controller.animation.renva.move": {
      "states": {
        "default": {
          "parameters": [ "query.is_moving && query.is_on_ground" ],
          "animations": [
            {
              "move": [
                {
                  "0.0": 0.0,
                  "1.0": 1.0
                }
              ]
            }
          ]
        }
      }
    },
    "controller.animation.renva.sit": {
      "states": {
        "default": {
          "parameters": [ "query.is_sitting && !query.is_moving && query.is_on_ground" ],
          "animations": [
