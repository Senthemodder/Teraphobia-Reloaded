        }
       },
      "minecraft:entity_spawned": {
				"add": {
         "component_groups": [
          "minecraft:exploding"
         ]
        },
				
				"queue_command": {
					"command": [
						"effect @s strength 70 4",
						"particle minecraft:totem_particle ~~~",
						"particle minecraft:totem_particle ~~1~",
						"particle minecraft:totem_particle ~1~~",
						"particle minecraft:totem_particle ~-1~~",
						"particle minecraft:totem_particle ~~~1",
						"particle minecraft:totem_particle ~~~-1",
						"playsound random.totem @a[r=35] ~~~"
					],
"target":"other"
				}
			}
		}
